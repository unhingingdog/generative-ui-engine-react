      "types": "./dist/template-utils.d.ts",
      "import": "./dist/template-utils.js"
    }
  },
  "dependencies": {
    "@openai/agents": "^0.0.16",
    "@openai/agents-openai": "^0.0.16",
    "eventsource-client": "^1.1.4",
