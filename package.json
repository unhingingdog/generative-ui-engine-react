    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsdown": "^0.12.9",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
