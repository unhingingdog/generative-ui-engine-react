{
  "name": "shadowboxin",
  "version": "0.1.0-beta.2",
  "description": "Utilities for building streaming, AI-generatied UIs.",
  "author": "Hamish Gilkison <hamishgilkison@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unhingingdog/generative-ui-engine-react.git"
  },
  "keywords": [
    "react",
    "generative-ui",
    "ai",
    "llm",
    "streaming"
  ],
