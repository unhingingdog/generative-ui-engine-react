      "types": "./dist/engine/index.d.ts",
      "import": "./dist/engine/index.js",
      "require": "./dist/engine/index.cjs"
    },
    "./template-utils": {
      "types": "./dist/template-utils/index.d.ts",
      "import": "./dist/template-utils/index.js",
      "require": "./dist/template-utils/index.cjs"
    },
    "./template-models": {
      "types": "./dist/template-models/index.d.ts",
      "import": "./dist/template-models/index.js",
      "require": "./dist/template-models/index.cjs"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsdown",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "telomere": "^0.1.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.3.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.3.0",
    "jsdom": "^24.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsdown": "^0.2.2",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
